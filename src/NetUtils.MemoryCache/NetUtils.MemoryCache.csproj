<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net45;</TargetFrameworks>
    <DebugType>pdbonly</DebugType>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <WarningsAsErrors />
  </PropertyGroup>

  <PropertyGroup>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <Authors>hanabi1224@github.com</Authors>
    <Description>Yet another memory cache library that supports reloading data in background</Description>
    <Copyright>Copyright (C) 2018 hanabi1224@github.com</Copyright>
    <PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/hanabi1224/NetUtils.MemoryCache</PackageProjectUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>memory cache,cache,MemoryCache</PackageTags>
    <Version>1.0.0-pre1</Version>
  </PropertyGroup>
    
  <ItemGroup>
    <PackageReference Include="SonarAnalyzer.CSharp" Version="7.1.0.5212">
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
  </ItemGroup>

</Project>
